{
    "version": 2,
    "buildCommand": "node vercel-build.js",
    "outputDirectory": "apps/web/.next",
    "installCommand": "echo 'Using custom build process'",
    "framework": "nextjs",
    "builds": [
        {
            "src": "apps/web/package-vercel.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ]
}