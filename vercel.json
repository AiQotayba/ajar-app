{
    "version": 2,
    "buildCommand": "turbo run build --filter=web",
    "outputDirectory": "apps/web/.next",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ]
}